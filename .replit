run = "python3 -m pip install --user -r requirements.txt > /dev/null 2>&1 && python3 main.py"
language = "python3"
entrypoint = "main.py"

[nix]
channel = "stable-23_11"

[deployment]
run = ["python", "main.py"]
deploymentTarget = "cloudrun"

[env]
PYTHONUNBUFFERED = "1"
REPLIT = "true"
SUPPRESS_SELENIUM_WARNINGS = "true"